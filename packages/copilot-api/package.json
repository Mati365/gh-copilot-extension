{
  "name": "@gh-copilot-ext/copilot-api",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "vite build --config ../../config/vite.build.config.ts",
    "build:dev": "NODE_ENV=development vite build --config ../../config/vite.build.config.ts --mode development",
    "test": "vitest --run --coverage --passWithNoTests --config ../../config/vite.build.config.ts",
    "test:watch": "vitest --watch --passWithNoTests --config ../../config/vite.build.config.ts",
    "lint": "eslint --max-warnings=0 src/",
    "check:types": "tsc --noEmit"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "dependencies": {
    "@gh-copilot-ext/commons": "*",
    "deep-object-diff": "^1.1.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
